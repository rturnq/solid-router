{
  "name": "@rturnq/solid-router",
  "version": "0.3.0-rc.1",
  "description": "A router for solid-js",
  "author": "Ryan Turnquist",
  "license": "MIT",
  "homepage": "https://github.com/rturnq/solid-router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rturnq/solid-router"
  },
  "type": "module",
  "exports": {
    ".": {
      "solid": "./dist/index.js",
      "default": "./dist/build/index.js"
    }
  },
  "main": "dist/build/index.cjs.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c&&tsc",
    "prebuild": "rimraf dist",
    "test": "jest && npm run test:types",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage && npm run test:types",
    "test:types": "tsc --project tsconfig.test.json",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "regexparam": "^1.3.0"
  },
  "peerDependencies": {
    "solid-js": ">= 1.0.0-rc.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/jest": "^26.0.16",
    "babel-preset-solid": "1.0.0-rc.7",
    "history": "^5.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.34.2",
    "rollup-plugin-filesize": "^9.1.0",
    "solid-jest": "^0.0.3",
    "solid-js": "1.0.0-rc.8",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2"
  },
  "jest": {
    "preset": "solid-jest/preset/browser"
  }
}
